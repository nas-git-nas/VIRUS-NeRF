<launch>

    <include file="$(find timed_roslaunch)/launch/timed_roslaunch.launch">
        <arg name="time" value="0" />
        <arg name="pkg" value="sensors" />
        <arg name="file" value="rs_cam1.launch" />
        <arg name="node_name" value="timed_roslaunch1" />
    </include>
